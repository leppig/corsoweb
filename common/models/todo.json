{
  "name": "todo",
  "plural": "todo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "dueto": {
      "type": "date",
      "default": "2100-02-28T00:00:00.000Z"
    },
    "place": {
      "type": "string"
    },
    "done": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "important": {
      "type": "boolean",
      "default": false
    }, 
    "extra": {
      "type": "string",
      "default": "pippero"
    }

  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
